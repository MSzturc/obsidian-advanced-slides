{
	"categories": [
		{
			"title": "## ğŸš€ Features",
			"labels": [
				"feature",
				"feat"
			]
		},
		{
			"title": "## ğŸ› Fixes",
			"labels": [
				"fix",
				"bugfix"
			]
		},
		{
			"title": "## ğŸ“š  Documentation",
			"labels": [
				"docs",
				"documentation"
			]
		},
		{
			"title": "## ğŸ“  Other",
			"labels": [
				"refactor",
				"revert",
				"build",
				"ci"
			]
		}
	],
	"ignore_labels": [
		"ignore"
	],
	"sort": "ASC",
	"template": "${{CHANGELOG}}\n\n**Full Changelog**: https://github.com/MSzturc/obsidian-advanced-slides/compare/${{FROM_TAG}}...${{TO_TAG}}",
	"pr_template": "- ${{TITLE}}\n",
	"empty_template": "- no changes",
	"label_extractor": [
		{
			"pattern": "^(feat|feature|fix|bugfix|docs|documentation|refactor|revert|build|ci):(.*)",
			"target": "$1"
		}
	],
	"duplicate_filter": {
		"pattern": "\\[ABC-....\\]",
		"on_property": "title",
		"method": "match"
	}
}
